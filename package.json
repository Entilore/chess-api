{
  "name": "chess-api",
  "version": "0.0.0",
  "private": true,
  "nyc": {
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "include": [
      "model/**/*.js"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "start": "node ./bin/www",
    "build": "babel index.js -d src",
    "test": "npm run test-unit",
    "test-unit": "cross-env NODE_ENV=test nyc --all mocha test/*.spec.js",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "jade": "~1.11.0",
    "morgan": "~1.8.1",
    "serve-favicon": "~2.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.0.1",
    "co-mocha": "^1.2.0",
    "cross-env": "^5.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "nyc": "^11.0.1",
    "sinon": "^2.3.2"
  }
}
